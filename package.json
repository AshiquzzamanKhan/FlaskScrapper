{
  "name": "FacebookPostAnanlyzer",
  "version": "2.0.0",
  "description": "> A simple flask app integrated with selenium to run over facebook to mine data\r > also using embedded twisted reactor along with gunicorn WSGI server for Heorku Usage",
  "main": "./frontend/reactapp.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "app": "python app.py",
    "watch": "webpack --progress -d --config webpack.config.js --watch",
    "build": "webpack -p --progress --config webpack.config.js",
    "dev-build": "webpack --progress -d --config webpack.config.js",
    "start": "npm-run-all --parallel dev-build app"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "npm": "^6.4.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "npm-run-all": "^4.1.3",
    "style-loader": "^0.23.1",
    "webpack": "^3.0.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
